.file-icon-container.data-v-b065ef00 {
  width: 70rpx;
  height: 70rpx;
  border-radius: 12rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-shrink: 0;
}
.file-0 .file-icon-container.data-v-b065ef00 {
