}
.highlight.data-v-b065ef00 {
  border-left: 5rpx solid #3b82f6;
  background-color: #e0f2fe;
}

/* 图片网格 */
.image-grid.data-v-b065ef00 {
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  gap: 20rpx;
  margin: 10rpx 0;
}
.grid-image.data-v-b065ef00 {
  width: 100%;
  border-radius: 12rpx;
  box-shadow: 0 2rpx 8rpx rgba(0, 0, 0, 0.1);
  transition: -webkit-transform 0.3s ease;
  transition: transform 0.3s ease;
  transition: transform 0.3s ease, -webkit-transform 0.3s ease;
}
.grid-image.data-v-b065ef00:hover {
  -webkit-transform: scale(1.02);
          transform: scale(1.02);
}

/* 项目小组样式 */
.team-grid.data-v-b065ef00 {
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  gap: 20rpx;
  margin: 10rpx 0;
}
.team-card.data-v-b065ef00 {
  background-color: #f0f9ff;
  border-radius: 12rpx;
  padding: 25rpx;
  text-align: center;
  transition: all 0.3s ease;
  border: 1rpx solid #dbeafe;
}
.team-card.data-v-b065ef00:hover {
  -webkit-transform: translateY(-3rpx);
          transform: translateY(-3rpx);
  box-shadow: 0 6rpx 16rpx rgba(147, 197, 253, 0.2);
  background-color: #e6f4ff;
}
.team-icon.data-v-b065ef00 {
  font-size: 40rpx;
  margin-bottom: 15rpx;
  display: inline-block;
  color: #3b82f6;
}
.team-title.data-v-b065ef00 {
  display: block;
  font-size: 26rpx;
  font-weight: bold;
  color: #1e40af;
  margin-bottom: 10rpx;
}
.team-desc.data-v-b065ef00 {
  font-size: 22rpx;
  color: #334155;
  line-height: 1.5;
}

/* 时间线样式 */
.timeline.data-v-b065ef00 {
  position: relative;
  margin: 20rpx 0;
  padding-left: 30rpx;
}
.timeline.data-v-b065ef00::before {
  content: '';
  position: absolute;
  left: 10rpx;
  top: 0;
  bottom: 0;
  width: 2rpx;
  background-color: #93c5fd;
  opacity: 0.5;
}
.timeline-item.data-v-b065ef00 {
  position: relative;
  margin-bottom: 30rpx;
}
.timeline-dot.data-v-b065ef00 {
  position: absolute;
  left: -30rpx;
  top: 5rpx;
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  background-color: #3b82f6;
  box-shadow: 0 0 0 5rpx rgba(59, 130, 246, 0.1);
}
.timeline-content.data-v-b065ef00 {
  background-color: #f0f9ff;
  border-radius: 12rpx;
  padding: 20rpx;
  transition: all 0.3s ease;
}
.timeline-content.data-v-b065ef00:hover {
  box-shadow: 0 4rpx 12rpx rgba(147, 197, 253, 0.2);
}
.timeline-title.data-v-b065ef00 {
  display: block;
  font-size: 26rpx;
  font-weight: bold;
  color: #1e88e5;
  margin-bottom: 10rpx;
}
.timeline-text.data-v-b065ef00 {
  font-size: 24rpx;
  color: #334155;
  line-height: 1.6;
}

/* 文件列表样式 */
.file-list.data-v-b065ef00 {
