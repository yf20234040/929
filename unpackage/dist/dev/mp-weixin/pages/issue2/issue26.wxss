}

/* 内容区块样式 */
.content-block.data-v-b065ef00 {
  display: flex;
  flex-direction: column;
  gap: 20rpx;
}
.content-card.data-v-b065ef00 {
  background-color: #f0f9ff;
  border-radius: 12rpx;
  padding: 25rpx;
  line-height: 1.6;
  border-left: 4rpx solid transparent;
  transition: all 0.3s ease;
}
.content-card.data-v-b065ef00:hover {
  box-shadow: 0 4rpx 12rpx rgba(147, 197, 253, 0.2);
}
.content-title.data-v-b065ef00 {
  display: block;
  font-size: 28rpx;
  font-weight: bold;
  color: #1e88e5;
  margin-bottom: 10rpx;
}
.content-text.data-v-b065ef00 {
