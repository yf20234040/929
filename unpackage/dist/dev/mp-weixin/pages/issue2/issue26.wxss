}
.btn-arrow.data-v-b065ef00 {
  color: #60a5fa;
  font-size: 24rpx;
  transition: -webkit-transform 0.3s ease;
  transition: transform 0.3s ease;
  transition: transform 0.3s ease, -webkit-transform 0.3s ease;
}
.file-card:hover .btn-arrow.data-v-b065ef00 {
  -webkit-transform: translateX(4rpx);
          transform: translateX(4rpx);
  color: #3b82f6;
}

/* 底部样式 */
.bottom-decoration.data-v-b065ef00 {
  margin-top: 60rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-bottom: 40rpx;
}
.decoration-dots.data-v-b065ef00 {
  display: flex;
  gap: 16rpx;
  margin-bottom: 30rpx;
}
.dot.data-v-b065ef00 {
  width: 10rpx;
  height: 10rpx;
  background: #93c5fd;
  border-radius: 50%;
  -webkit-animation: pulse-data-v-b065ef00 2.5s infinite;
          animation: pulse-data-v-b065ef00 2.5s infinite;
}
.dot.data-v-b065ef00:nth-child(2) {
  -webkit-animation-delay: 0.5s;
          animation-delay: 0.5s;
}
.dot.data-v-b065ef00:nth-child(3) {
  -webkit-animation-delay: 1s;
          animation-delay: 1s;
}
@-webkit-keyframes pulse-data-v-b065ef00 {
0%, 100% {
    opacity: 0.6;
    -webkit-transform: scale(1);
            transform: scale(1);
}
50% {
    opacity: 1;
    -webkit-transform: scale(1.15);
            transform: scale(1.15);
}
}
@keyframes pulse-data-v-b065ef00 {
0%, 100% {
    opacity: 0.6;
    -webkit-transform: scale(1);
            transform: scale(1);
}
50% {
    opacity: 1;
    -webkit-transform: scale(1.15);
            transform: scale(1.15);
}
}
.footer.data-v-b065ef00 {
  text-align: center;
  padding: 25rpx 0;
  color: #64748b;
  margin-top: 20rpx;
}
.organizer.data-v-b065ef00 {
