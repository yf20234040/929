}
.file-icon.data-v-b065ef00 {
  font-size: 32rpx;
}
.file-0 .file-icon.data-v-b065ef00 {
  color: #3b82f6;
}
.file-1 .file-icon.data-v-b065ef00 {
  color: #0ea5e9;
}
.file-2 .file-icon.data-v-b065ef00 {
  color: #60a5fa;
}

/* 文件信息区域 */
.file-info.data-v-b065ef00 {
  flex: 1;
  min-width: 0;
}
.file-name.data-v-b065ef00 {
  display: block;
  font-size: 28rpx;
  font-weight: 500;
  color: #1e293b;
  margin-bottom: 8rpx;
  line-height: 1.4;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.file-meta.data-v-b065ef00 {
  display: flex;
  align-items: center;
  gap: 10rpx;
  font-size: 22rpx;
}
.file-type.data-v-b065ef00 {
  color: #3b82f6;
  background: #eff6ff;
