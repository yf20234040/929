  transition: all 0.3s ease;
  cursor: pointer;
  display: flex;
  align-items: center;
  gap: 24rpx;
  border: 1rpx solid #e0f2fe;
}
.file-card.data-v-b065ef00:hover {
  -webkit-transform: translateY(-2rpx);
          transform: translateY(-2rpx);
  box-shadow: 0 6rpx 16rpx rgba(147, 197, 253, 0.2);
  border-color: #bfdbfe;
  background-color: #f8fafc;
}

/* 不同文件卡片的主题色边框 */
.file-0.data-v-b065ef00 {
